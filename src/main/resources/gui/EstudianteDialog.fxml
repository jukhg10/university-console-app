<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.GridPane?>
<DialogPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <headerText>Formulario para Nuevo Estudiante</headerText>
    <content>
        <GridPane hgap="10" vgap="10">
            <padding><Insets top="20" right="20" bottom="10" left="10" /></padding>
            <Label text="Nombres:" GridPane.rowIndex="0" GridPane.columnIndex="0" />
            <TextField fx:id="nombresField" promptText="Ej: Ana" GridPane.rowIndex="0" GridPane.columnIndex="1" />
            <Label text="Apellidos:" GridPane.rowIndex="1" GridPane.columnIndex="0" />
            <TextField fx:id="apellidosField" promptText="Ej: Gomez" GridPane.rowIndex="1" GridPane.columnIndex="1" />
            <Label text="Email:" GridPane.rowIndex="2" GridPane.columnIndex="0" />
            <TextField fx:id="emailField" promptText="Ej: ana@email.com" GridPane.rowIndex="2" GridPane.columnIndex="1" />
            <Label text="CÃ³digo:" GridPane.rowIndex="3" GridPane.columnIndex="0" />
            <TextField fx:id="codigoField" promptText="Ej: 202401" GridPane.rowIndex="3" GridPane.columnIndex="1" />
        </GridPane>
    </content>
    <buttonTypes>
        <ButtonType text="Guardar" buttonData="OK_DONE" />
        <ButtonType text="Cancelar" buttonData="CANCEL_CLOSE" />
    </buttonTypes>
</DialogPane>